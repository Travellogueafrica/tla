<div class="page-content">
    <div class="container-fluid">

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                    <h4 class="mb-sm-0 font-size-18">Subscribers</h4>

                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">Dashboard</a></li>
                            <li class="breadcrumb-item active">Subscribers</li>
                        </ol>
                    </div>

                </div>
            </div>
        </div>
        <!-- end page title -->
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table align-middle table-nowrap table-hover">
                                <thead class="table-light">
                                    <tr>
                                        <th scope="col" style="width: 70px;">#</th>
                                        <th scope="col">Email</th>
                                        <th scope="col">Country</th>
                                        <th scope="col">Region</th>
                                        <th scope="col">Status</th>
                                        <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (members.length){ %>
                                        <% members.forEach(function(user){ %>
                                            <tr>
                                                <td>
                                                    <div class="avatar-xs">
                                                        <span class="avatar-title rounded-circle">
                                                            D
                                                        </span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <h5 class="font-size-14 mb-1"><a href="javascript: void(0);" class="text-dark"><%- user.email %></a></h5>
                                                    <p class="text-muted mb-0"><%- user.ip_address %></p>
                                                </td>
                                                <td><%- user.country == null ? 'n/a' : user.country %></td>
                                                <td><%- user.region == null ? 'n/a' : user.region %></td>
                                                <td><span class="badge badge-pill badge-soft-<%- user.status ? 'success' : 'danger' %> font-size-12"><%- user.status ? 'Enabled' : 'Disabled' %></span></td>
                                                <td>
                                                    <ul class="list-inline font-size-20 contact-links mb-0"> 
                                                        <li class="list-inline-item px-2">
                                                            <a href="javascript: void(0);" title="Blacklist"><i class="bx bx-user-circle"></i></a>
                                                        </li>
                                                    </ul>
                                                </td>
                                            </tr>
                                        <% }) %>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <% if (pages > 1) { %>
                                    <ul class="pagination pagination-rounded justify-content-center mt-4">
                                        <% if (current == 1) { %>
                                        <li class="page-item disabled">
                                            <a class="page-link" href="javascript: void(0);" aria-label="Previous">
                                                <i class="mdi mdi-chevron-left"></i>
                                            </a>
                                        </li>
                                        <% } else { %>
                                        <li class="page-item">
                                            <a class="page-link" href="/admin/subscribers/<%- parseInt(current) - 1 %>" aria-label="Previous">
                                                <i class="mdi mdi-chevron-left"></i>
                                            </a>
                                        </li>
                                        <% } %>
                                        <% let i = (parseInt(current) > 8 ? parseInt(current) - 8 : 1) %>
                                        <% if (i !== 1) { %>
                                            <li style="display:none"><a href="#">...</a></li>
                                        <% } %>
                                        <% for (; i <= (parseInt(current) + 7) && i <= pages; i++) { %>
                                            <% if (i == current) { %>
                                                <li class="page-item active"><a class="page-link" href="javascript: void(0);"><%- i %></a></li>
                                            <% } else { %>
                                                <li class="page-item"><a class="page-link" href="/admin/subscribers/<%- i %>"><%- i %></a></li>
                                                <% } %>
                                            <% } %>
                                            <% if (i == parseInt(current) + 7 && i < pages) {%>
                                                <li class="disabled"><a href="#">...</a></li>
                                        <% } %>
                                        <% if (current < pages){ %>
                                            <li class="page-item">
                                                <a class="page-link" href="/admin/subscribers/<%- parseInt(current) + 1 %>" aria-label="Next">
                                                    <i class="mdi mdi-chevron-right"></i>
                                                </a>
                                            </li>
                                        <% } %>
                                    </ul>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>